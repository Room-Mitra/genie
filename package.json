{
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,json,css,md}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write",
    "*.tf": [
      "terraform fmt -recursive"
    ]
  },
  "scripts": {
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "nvm": "^0.0.4"
  }
}
